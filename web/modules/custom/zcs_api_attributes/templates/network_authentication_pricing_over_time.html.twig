<div>
	{{ content.link }}
	<table class="attributes-table">
		<thead>
			<tr>
			    <th>#</th>
				<th>API Attribute</th>
				{% for date , atts in content.header_date %}
					<th>
						Price Effective<br>{{ atts }}</th>
				{% endfor %}
			</tr>
		</thead>
		<tbody>
		{% set counter = 1 %}
			{% for key, att in content.final %}
				<tr>
					<td>{{ counter }}</td>
					<td>{{ att.title }}</td>
					{% for key, date in content.header_date %}
						<td>
							{{ content.symbols[key] }}
							{{ att['price' ~ key] }}</td>
					{% endfor %}
				</tr>
					{% set counter = counter + 1 %}
			{% endfor %}
					
		</tbody>
	</table>
</div>
