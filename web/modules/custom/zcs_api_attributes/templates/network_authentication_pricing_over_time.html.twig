<div>
  <div class="user-management-view api-attribute-status-header">
    <header>
      {{ content.link|merge({'#attributes': {'class': ['btn-primary']}}) }}
    </header>
  </div>
  <table class="attributes-table">
    <thead>
      <tr>
        <th>#</th>
        <th>API Attributes</th>
        {% for date , atts in content.header_date %}
          <th>Price Effective<br>{{ atts }}<br>International / Domestic</th>
        {% endfor %}
      </tr>
    </thead>
    <tbody>
      {% set counter = 1 %}
      {% for key, att in content.final %}
        <tr>
          <td>{{ counter }}</td>
          <td>{{ att.title }}</td>
          {% for key, date in content.header_date %}
            <td>
              {{ content.symbols[key] }} {{ att['international_price_' ~ key] }} / {{ content.symbols[key] }} {{ att['domestic_price_' ~ key] }}
            </td>
          {% endfor %}
        </tr>
        {% set counter = counter + 1 %}
      {% endfor %}

    </tbody>
  </table>
</div>
