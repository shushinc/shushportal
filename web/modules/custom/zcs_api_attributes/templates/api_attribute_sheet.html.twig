{{ form.form_build_id }}
{{ form.form_token }}
{{ form.form_id }}

{% if form.message %}
	<p class="message">{{ form.message }}</p>
{% endif %}
<div>
	<h2 class="attributes-label">API Attribute status</h2>
	<span class="attribute-inputs">
		<table class="attributes-table">
			<thead>
				<tr>
					<th>Title</th>
					<th>Network Connected</th>
					<th>Able to be used</th>
				</tr>
			</thead>
			<tbody>
				{% for id, title in fetch_attributes() %}
					<tr>
						<td>{{ title }}</td>
						<td>{{ form['network_connected' ~ id] }}</td>
						<td>{{ form['current_standard_price' ~ id] }}</td>
					</tr>
				{% endfor %}
			</tbody>
		</table>
	</span>
</div>


{{ form.submit }}
