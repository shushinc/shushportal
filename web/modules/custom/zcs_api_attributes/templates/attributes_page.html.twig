<div>
	{{ content.link }}
	<table class="attributes-table">
		<thead>
			<tr>
				<th>#</th>
				<th>API Attribute</th>
				<th>Network Connected</th>
				<th>Able to be used</th>
			</tr>
		</thead>
		<tbody>
			{% set counter = 1 %}
			{% for mainkey, attributes in content.final %}
				{% for key, att in attributes %}
					<tr>
						<td>{{ counter }}</td>
						<td>{{ att.title }}</td>
						<td>
							{{ att.integrated_carrier_network }}
						</td>
						<td>
							{{ att.to_be_used }}
						</td>
					</tr>
					{% set counter = counter + 1 %}
				{% endfor %}
			{% endfor %}
		</tbody>
	</table>
</div>
